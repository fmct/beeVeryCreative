                        color: 1,1,1,1
                        on_press: app.putNum(self.text)
                    Button:
                        text: '7'
                        font_size: 22
                        color: 1,1,1,1
                        on_press: app.putNum(self.text)
                    Button:
                        text: '8'
                        font_size: 22
                        color: 1,1,1,1
                        on_press: app.putNum(self.text)
                    Button:
                        text: '9'
