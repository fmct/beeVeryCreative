<label>:
    color: 0,0,0,1
    font_size: 25

ShowcaseScreen:
    id: ola
    name: 'ScreenManager'
    fullscreen: True
    ScreenManager:
        id: sm

        Screen:
            name: 'screen1'
            canvas.before:
                Rectangle:
                    size: self.size
            RelativeLayout:
                Image:
                    source: 'data/bee_burned.png'
                    pos_hint: {'center_x': 0.1, 'center_y': 0.88}
                    size_hint: 0.2,0.21
                Label:
                    id: printerList
                    bold: True
                    font_size: 35
                    text: 'Printers list'
                    color: 0,0,0,1
                    pos_hint: {'center_x': 0.5, 'center_y': 0.85}
                GridLayout:
                    id: grid
                    spacing: '5dp'
                    pos_hint: {'center_x': 0.5, 'center_y': 0.4}
                    size_hint: 0.9,0.7
                    cols: 3
                    Button:
                        id: firstPrinter
                        color: 1,1,1,1
                        width: 100
                        opacity: 0.8
                        text: 'Empty'
                        font_size: 20
                        text_size: cm(5), cm(2)
                        valign: 'middle'
                        on_press: app.selectButton1 = True
                        on_release: app.chooseScreen(self.text)
                        halign: 'center'
                    Button:
                        id: secondPrinter
                        color: 1,1,1,1
                        width: 100
                        font_size:20
                        text: 'Empty'
                        opacity: 0.8
                        text_size: cm(5), cm(2)
                        valign: 'middle'
                        halign: 'center'
                        on_press: app.selectButton2 = True
                        on_release: app.chooseScreen(self.text)
                    Button:
                        id: thirdPrinter
                        color: 1,1,1,1
                        width: 100
                        font_size:20
                        text: 'Empty'
                        opacity: 0.8
                        text_size: cm(5), cm(2)
                        valign: 'middle'
                        halign: 'center'
                        on_press: app.selectButton3 = True
                        on_release: app.chooseScreen(self.text)
                    Button:
                        id: fourPrinter
                        color: 1,1,1,1
                        width:100
                        font_size:20
                        text: 'Empty'
                        opacity: 0.8
                        text_size: cm(5), cm(2)
                        valign: 'middle'
                        halign: 'center'
                        on_press: app.selectButton4 = True
                        on_release: app.chooseScreen(self.text)
        Screen:
            name: 'screen16'
            canvas.before:
                Rectangle:
                    size: self.size
            RelativeLayout:
                id: relativeLayout
                Image:
                    source: 'data/bee_burned.png'
                    pos_hint: {'center_x': 0.1, 'center_y': 0.9}
                    size_hint: 0.2,0.21
                Button:
                    id: search_new_pic
